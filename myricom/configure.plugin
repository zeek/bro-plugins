#!/bin/sh
#

plugin_usage() {
    cat <<EOF
    --with-myricom=DIR         Path to Myricom installation
EOF
}

plugin_option() {

    case "$1" in
        --with-myricom=*)
            append_cache_entry MYRICOM_ROOT_DIR PATH $optarg
            return 0
            ;;

        *)
            return 1;
            ;;
    esac
}
